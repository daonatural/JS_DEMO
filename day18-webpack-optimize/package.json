    "@babel/preset-react": "^7.8.0",
    "@babel/preset-stage-0": "^7.8.0",
    "add-asset-html-webpack-plugin": "^3.1.3",
    "babel-loader": "^8.0.6",
    "cross-env": "^6.0.3",
    "css-loader": "^3.4.2",
    "glob": "^7.1.6",
    "happypack": "^5.0.1",
    "html-webpack-plugin": "^3.2.0",
    "mini-css-extract-plugin": "^0.9.0",
    "purgecss-webpack-plugin": "^1.6.0",
    "thread-loader": "^2.1.3",
    "webpack": "^4.41.5",
    "webpack-cli": "^3.3.10",
    "webpack-merge": "^4.2.2"
  },
  "dependencies": {
