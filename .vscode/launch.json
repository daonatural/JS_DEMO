{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "local node demo",
      "program": "${workspaceFolder}/test.js",
      "skipFiles": [
        "<node_internals>/**"
      ]
    },
    {
      "type": "node",
      "request": "launch",
      "name": "debug webpack",
      "cwd": "${workspaceFolder}/day19-webpack-core",
      "program": "${workspaceFolder}/day19-webpack-core/node_modules/webpack-cli/bin/cli.js",
      "args": [
        "--config",
        "${workspaceFolder}/day19-webpack-core/webpack.config.js"
      ]
    },
    {
      "type": "node",
      "request": "launch",
      "name": "debug test",
      "program": "${workspaceFolder}/js架构散述/06-数据结构概论/code/practice1.js"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "debug koa",
      "program": "${workspaceFolder}/day5.2-koa/self-koa/self-applications.js"
    },
    {
      "type": "node-terminal",
      "name": "Create JavaScript Debug Terminal",
      "request": "launch",
      "cwd": "${workspaceFolder}"
    }
  ]
}